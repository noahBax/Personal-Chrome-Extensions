{
"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["DataOrganizer.js","PageSearcher.js"],
		"css": ["HowTextLooks.css"],
		"run_at": "document_end",
		"all_frames": true
	}
	],
  "manifest_version": 2,
  "name": "Quizlet Auto-Fill",
  "version": "1.0.0",
  "browser_action": {
	"default_popup": "Popup.html",
	"browser_style": true,
    "default_icon": "StickyNote.png",
	"default_title": "Quizlet Auto-Fill"
   },
  "icons": {
   "48": "StickyNote.png",
   "96": "StickyNote.png"
   },
	"background": {
		"scripts": ["content.js"]
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"content_security_policy": 
		"script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'",
	"web_accessible_resources": [
		"Thumbtack.png"
	]
}